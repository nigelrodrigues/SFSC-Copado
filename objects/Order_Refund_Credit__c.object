    <startsWith>Vowel</startsWith>
    <validationRules>
        <fullName>Price_Override_Reason</fullName>
        <active>true</active>
        <description>Must select price override reason if a price override amount is entered.</description>
        <errorConditionFormula>ispickval( Reason__c ,&quot;Price Override&quot;) &amp;&amp;   isblank(Text(Price_Override_Reason__c))</errorConditionFormula>
        <errorDisplayField>Price_Override_Reason__c</errorDisplayField>
        <errorMessage>Must select a Price Override Reason (Error ORC-01)</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Support_Agent_Cant_Approve_ORCs</fullName>
        <active>true</active>
        <description>Validation Rule prevents ORC Status from being changed to &apos;Closed&apos; by HBC Support Agents.</description>
        <errorConditionFormula>ISPICKVAL( ORC_Status__c , &apos;Approved&apos;) &amp;&amp;
ISCHANGED(ORC_Status__c ) &amp;&amp;
NOT(OR(
(CONTAINS($Profile.Name, &apos;Service Excellence Agent&apos;)),
(CONTAINS($Profile.Name, &apos;Executive Services&apos;)),
(CONTAINS($Profile.Name, &apos;Support Management&apos;)),
(CONTAINS($Profile.Name, &apos;Support Team Lead&apos;)),
(CONTAINS($Profile.Name, &apos;Administrator&apos;))
))</errorConditionFormula>
        <errorDisplayField>ORC_Status__c</errorDisplayField>
        <errorMessage>Only the Service Excellence Team is allowed to approve ORC&apos;s (Error ORC-03)</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Support_Agent_Cant_Close_ORCs</fullName>
        <active>true</active>
        <description>Validation Rule prevents ORC Status from being changed to &apos;Closed&apos; by HBC Support Agents.</description>
        <errorConditionFormula>( ISPICKVAL( ORC_Status__c , &apos;Closed&apos;) ) &amp;&amp;(CONTAINS($Profile.Name, &apos;Support Agent&apos;))</errorConditionFormula>
        <errorDisplayField>ORC_Status__c</errorDisplayField>
        <errorMessage>Only the POS Team is allowed to close ORC&apos;s once they have been processed (Error ORC-02)</errorMessage>
    </validationRules>
    <visibility>Public</visibility>
</CustomObject>

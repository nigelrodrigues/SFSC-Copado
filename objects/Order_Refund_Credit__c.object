    <fields>
        <fullName>Adjusted_Price__c</fullName>
        <description>Price to be used for adjustments. Used in ORC Amount to Refund</description>
        <externalId>false</externalId>
        <formula>if(ispickval( Reason__c , &quot;Order Not Received&quot;)=TRUE,0,if(Price_Override__c&gt;0,Price_Override__c, Amount_Charged__c ))- (Amount_Charged__c * Discount__c) -  Manual_Adjustment__c</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>Adjusted Price</label>
        <precision>18</precision>
        <required>false</required>
        <scale>2</scale>
        <trackHistory>false</trackHistory>
        <trackTrending>false</trackTrending>
        <type>Currency</type>
    </fields>

    <fields>
        <fullName>Amount_to_Refund__c</fullName>
        <externalId>false</externalId>
        <formula>if(OR(ispickval( Reason__c , &quot;Order Not Received&quot;)=TRUE, Quantity__c = Quantity_Not_Received__c ), Amount_Charged__c,Amount_Charged__c - Adjusted_Price__c) + if( Credit_Shipping_Fee__c =TRUE, Shipping_Fee__c ,0)+Other_Refund__c</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>ORC Amount to Refund</label>
        <precision>18</precision>
        <required>false</required>
        <scale>2</scale>
        <trackHistory>false</trackHistory>
        <trackTrending>false</trackTrending>
        <type>Currency</type>
    </fields>
    <fields>
        <fullName>Approval_Required_Flag__c</fullName>
        <externalId>false</externalId>
        <formula>IF( Approval_Required__c , IMAGE( &quot;/img/samples/flag_red.gif&quot;, &quot;Approval Required&quot; ), &quot;&quot;)</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>Approval Required</label>
        <required>false</required>
        <trackHistory>false</trackHistory>
        <trackTrending>false</trackTrending>
        <type>Text</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>Approval_Required__c</fullName>
        <externalId>false</externalId>
        <formula>( Amount_to_Refund__c &gt;= 100 ||  Replacement_Gift_Card_Amount__c &gt;=100 || ISPICKVAL(Gift_Card_Amount__c, &quot;100&quot;) ) 
&amp;&amp; 

NOT(ISPICKVAL(ORC_Status__c, &apos;Approved&apos;) || ISPICKVAL(ORC_Status__c, &apos;Closed&apos;) || ISPICKVAL(ORC_Status__c, &apos;Cancelled&apos;) || ISPICKVAL(ORC_Status__c, &apos;Draft&apos;))</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>Approval Required</label>
        <trackHistory>false</trackHistory>
        <trackTrending>false</trackTrending>
        <type>Checkbox</type>

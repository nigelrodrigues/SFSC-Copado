    <validationRules>
        <fullName>Support_Agent_Cant_Give_Over_20</fullName>
        <active>true</active>
        <description>Limits the Promo Code % to max 20% for regular agents</description>
        <errorConditionFormula>Consumed__c &amp;&amp;
ISCHANGED( Consumed__c ) &amp;&amp; 
(
ISPICKVAL(Amount_List__c ,&quot;25%&quot;) ||
ISPICKVAL(Amount_List__c ,&quot;30%&quot;) ||
ISPICKVAL(Amount_List__c ,&quot;35%&quot;) ||
ISPICKVAL(Amount_List__c ,&quot;40%&quot;) ||
ISPICKVAL(Amount_List__c ,&quot;45%&quot;) ||
ISPICKVAL(Amount_List__c ,&quot;50%&quot;) ||
ISPICKVAL(Amount_List__c ,&quot;55%&quot;) ||
ISPICKVAL(Amount_List__c ,&quot;60%&quot;) ||
ISPICKVAL(Amount_List__c ,&quot;65%&quot;) ||
ISPICKVAL(Amount_List__c ,&quot;70%&quot;) ||
ISPICKVAL(Amount_List__c ,&quot;75%&quot;) ||
ISPICKVAL(Amount_List__c ,&quot;80%&quot;) ||
ISPICKVAL(Amount_List__c ,&quot;85%&quot;) ||
ISPICKVAL(Amount_List__c ,&quot;90%&quot;) ||
ISPICKVAL(Amount_List__c ,&quot;95%&quot;)
)
&amp;&amp;
NOT(OR( 
(CONTAINS($Profile.Name, &apos;Service Excellence Agent&apos;)), 
(CONTAINS($Profile.Name, &apos;Executive Services&apos;)), 
(CONTAINS($Profile.Name, &apos;Support Management&apos;)), 
(CONTAINS($Profile.Name, &apos;Support Team Lead&apos;)), 
(CONTAINS($Profile.Name, &apos;Administrator&apos;)) 
))</errorConditionFormula>
        <errorMessage>Promo Codes over 20% must be given by a team lead (Error Promo Code - 01)</errorMessage>
    </validationRules>

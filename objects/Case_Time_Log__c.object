    <fields>
        <fullName>Business_Hours_Duration_Days__c</fullName>
        <description>Converts Business_Hours_Duration__c (secs) to days</description>
        <externalId>false</externalId>
        <formula>Business_Hours_Duration__c / 86400</formula>

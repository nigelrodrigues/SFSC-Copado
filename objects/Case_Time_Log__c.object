    <fields>
        <fullName>Case_Status_Group__c</fullName>
        <description>This field allows categorization of the Case Status values into Customer or Company for reporting purposes.  Edit the formula below as required to account for the values in the Case Status field.</description>
        <externalId>false</externalId>
        <formula>/* EDIT THIS FORMULA AS REQUIRED TO MATCH THE CASE STATUS FIELD VALUES */
/* THIS IS AN EXAMPLE ONLY */
/*                         */
Case ( Case_Status__c , 
&quot;Team Assistance Needed&quot;,&quot;Company&quot;, 
&quot;Awaiting Service Excellence Review&quot;,&quot;Company&quot;, 
&quot;POS Team&quot;,&quot;Company&quot;, 
&quot;Pending Fraud Team&quot;,&quot;Company&quot;, 
&quot;System Issue&quot;,&quot;Company&quot;, 
&quot;Awaiting Customer Response&quot;,&quot;Customer&quot;, 
&quot;Awaiting Internal Response&quot;,&quot;Company&quot;, 
&quot;Response Received&quot;,&quot;Company&quot;, 
&quot;New&quot;,&quot;Company&quot;, 
&quot;In Progress&quot;,&quot;Company&quot;, 
&quot;Closed&quot;,&quot;Customer&quot;, 
&quot;Company&quot;)</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <inlineHelpText>This field allows categorization of the Case Status values into Customer or Company for reporting purposes.</inlineHelpText>
        <label>Case Status Group</label>
        <required>false</required>
        <trackTrending>false</trackTrending>
        <type>Text</type>
        <unique>false</unique>
    </fields>

    <fields>
        <fullName>Invalid_Contact__c</fullName>
        <description>Checks if the contact is only a phone number</description>
        <externalId>false</externalId>
        <formula>OR (
OR (
BEGINS(LastName, &quot;+&quot;),
BEGINS(LastName, &quot;1&quot;),
BEGINS(LastName, &quot;2&quot;),
BEGINS(LastName, &quot;3&quot;),
BEGINS(LastName, &quot;4&quot;),
BEGINS(LastName, &quot;5&quot;),
BEGINS(LastName, &quot;6&quot;),
BEGINS(LastName, &quot;7&quot;),
BEGINS(LastName, &quot;8&quot;),
BEGINS(LastName, &quot;9&quot;)
) &amp;&amp; ISBLANK(FirstName),
OR (
BEGINS(Phone, &quot;888&quot;),
BEGINS(Phone, &quot;800&quot;),
BEGINS(Phone, &quot;866&quot;),
BEGINS(Phone, &quot;877&quot;),
BEGINS(Phone, &quot;855&quot;)))</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>Invalid Contact</label>
        <trackHistory>false</trackHistory>
        <type>Checkbox</type>
    </fields>

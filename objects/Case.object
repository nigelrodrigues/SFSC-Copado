    <validationRules>
        <fullName>Case_01_ORC_100_plus_go_to_SE_team</fullName>
        <active>true</active>
        <errorConditionFormula>AND(
NOT(Trigger_Approval_Override__c),
ISCHANGED(Status),
ispickval(Status ,&quot;POS Team&quot;),
 ORC_Requiring_Approval_Count__c &gt; 0)
||
AND(
ISCHANGED(Status),
ispickval(Status ,&quot;Closed&quot;),
 ORC_Requiring_Approval_Count__c &gt; 0)</errorConditionFormula>
        <errorMessage>Order Refund/Credit above 100$ must be sent to the Service Excellence Team for approval before being sent to the POS Team or Closed (Validation Case - 01)</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Case_02_Prevent_Follow_Up_On_Roles</fullName>
        <active>true</active>
        <description>This validation rule prevents specific roles in the company from creating a follow up date.</description>
        <errorConditionFormula>OR($UserRole.Name = &quot;L+T &amp; Bay Digital - Cebu&quot;,
$UserRole.Name = &quot;Bay Digital - El Salvador&quot;, 
$UserRole.Name = &quot;Saks &amp; Off 5th - El Salvador&quot;) &amp;&amp;  OR(ISCHANGED(Follow_Up_Date__c),
AND(ISCHANGED(Status), ISPICKVAL(Status, &quot;Follow Up&quot;)),AND(ISNEW(), ISPICKVAL(Status, &quot;Follow Up&quot;)))</errorConditionFormula>
        <errorMessage>As a front line agent you cannot set a follow up date. (Validation Case - 02)</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Case_03_Escalating_POS_OR_MNR</fullName>
        <active>true</active>
        <description>Used to prevent certain users from escalating ORC POS or MNR Review</description>
        <errorConditionFormula>AND
(ISCHANGED( Status ), 
OR(
ISPICKVAL(Status, &apos;Escalate ORC POS Review&apos;),
ISPICKVAL(Status, &apos;Escalate ORC MNR Review&apos;)),
OR(
$Profile.Name = &apos;HBC Support Agent&apos;
,
$Profile.Name = &apos;HBC Service Excellence Agent&apos;
,
$Profile.Name = &apos;HBC Fraud Agent&apos; 
,
$Profile.Name = &apos;HBC POS Agent&apos;))</errorConditionFormula>
        <errorMessage>Only team leads and management can escalate a case awaiting POS or MNR Review (Validation Case - 03)</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Case_04_Close_Case_Without_Case_Type</fullName>
        <active>true</active>
        <description>Prevents a user from closing a case that doesn&apos;t have a type.</description>
        <errorConditionFormula>AND(ISCHANGED(Status),
Ispickval(Status,&quot;Closed&quot;),isblank(Text(Case_Type__c)), $User.ProfileId &lt;&gt;&quot;00e2E0000022NRy&quot;)</errorConditionFormula>
        <errorDisplayField>Case_Type__c</errorDisplayField>
        <errorMessage>You must select a Case Type before closing the case (Validation Case - 04)</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Case_05_Prevent_Close_Case_No_Contact</fullName>
        <active>true</active>
        <description>Validates if there is a contact on the case for some case types.</description>
        <errorConditionFormula>AND (Ispickval(Status, &quot;Closed&quot;),
$Profile.Name != &quot;System Administrator&quot;,
No_Contact__c = TRUE, 
NOT(OR(
Ispickval( Category__c, &quot;Customer asked for Store Information (phone number / details)&quot;),
Ispickval( Category__c, &quot;I need additional information details on a product&quot;),
Ispickval( Category__c, &quot;Inventory inquiry in store (website is out of stock)&quot;),
Ispickval( Category__c, &quot;Inventory inquiry on website&quot;),
Ispickval( Category__c, &quot;Transfer to CapOne&quot;),
Ispickval( Category__c, &quot;Transfer to Credit Services&quot;),
Ispickval( Category__c, &quot;Transfer to Store&quot;),
Ispickval( Category__c, &quot;Transfer to Total Guard/Vendor&quot;),
Ispickval( Category__c, &quot;What is your return policy?&quot;),
Ispickval( Category__c, &quot;Ghost Call&quot;)
)))</errorConditionFormula>
        <errorMessage>You can&apos;t close a case of this type without a valid contact. (Validation Case - 05)</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Case_06_Follow_Up_Date</fullName>
        <active>false</active>
        <description>Check if follow up date is blank on follow up status. Ensure that follow up date is not more than 30 days into the future. Ensures that the date is not set in the past.</description>
        <errorConditionFormula>ispickval (Status, &quot;Follow Up&quot;) &amp;&amp; OR(isblank( Follow_Up_Date__c ), Follow_Up_Date__c  &gt; (TODAY() +30), (OR(ISCHANGED(Follow_Up_Date__c),ISNEW()) &amp;&amp; Follow_Up_Date__c &lt; TODAY()))</errorConditionFormula>
        <errorDisplayField>Follow_Up_Date__c</errorDisplayField>
        <errorMessage>You must enter a Follow Up date if setting case to Follow Up Status. The date cannot be more than 30 days into the future. The date cannot be set in the past. (Validation Case - 06)</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Case_07_Cant_close_case_with_active_ORC</fullName>
        <active>true</active>
        <errorConditionFormula>AND(
  ISPICKVAL(Status, &quot;Closed&quot;),
  Is_ORC_Open__c,
  NOT(CONTAINS($Profile.Name,&quot;Administrator&quot;))
)</errorConditionFormula>
        <errorMessage>All ORC must be closed before the case can be closed.(Validation Case - 07)</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Case_08_TL_Reason_Required</fullName>
        <active>true</active>
        <errorConditionFormula>AND(TL_Review_Required__c ,  ISBLANK(TL_Review_Reason__c) )</errorConditionFormula>
        <errorDisplayField>TL_Review_Reason__c</errorDisplayField>
        <errorMessage>Please enter an appropriate TL Review reason to proceed. (Validation Case - 08)</errorMessage>
    </validationRules>

    <formulas>
        <name>Adjusted_Price_Price_Adjustment</name>
        <dataType>Currency</dataType>
        <expression>{!ORC_Amount_Charged}-({!ORC_Amount_Charged}*({!Form_Discount_Percent_Price_Adjustment}/100))-{!Form_Manual_Adjustment_Price_Adjustment}</expression>
        <scale>2</scale>
    </formulas>
    <formulas>
        <name>Adjusted_Price_Promo_Question</name>
        <dataType>Currency</dataType>
        <expression>{!ORC_Amount_Charged}-({!ORC_Amount_Charged}*({!Form_Discount_Percent_Promo}/100))-{!Form_Manual_Adjustment_Promo}</expression>
        <scale>2</scale>
    </formulas>
    <formulas>
        <name>Amount_Refund_Defective_Item</name>
        <dataType>Currency</dataType>
        <expression>IF({!Form_Discount_Percent_Defective_Item}&gt;0,{!ORC_Amount_Charged}*{!Form_Discount_Percent_Defective_Item}/100,0)+if({!Form_Manual_Adjustment_Defective_Item}&gt;0,{!Form_Manual_Adjustment_Defective_Item},0)+ {!ORC_Shipping_Fee}</expression>
        <scale>2</scale>
    </formulas>
    <formulas>
        <name>Amount_Refund_Promo</name>
        <dataType>Currency</dataType>
        <expression>{!ORC_Amount_Charged}-{!Adjusted_Price_Promo_Question}</expression>
        <scale>2</scale>
    </formulas>
    <formulas>
        <name>Amount_Refund_Promo_Code</name>
        <dataType>Currency</dataType>
        <expression>IF({!Form_Discount_Percent_Promo}&gt;0,({!ORC_Amount_Charged}*{!Form_Discount_Percent_Promo}/100),0)+
if({!Form_Manual_Adjustment_Promo}&gt;0,{!Form_Manual_Adjustment_Promo},0)+ {!ORC_Shipping_Fee}</expression>
        <scale>2</scale>
    </formulas>
    <formulas>
        <name>Amount_Refund_Return_Problem</name>
        <dataType>Currency</dataType>
        <expression>if({!Form_Discount_Percent_Return_Problem}&gt;0,({!ORC_Amount_Charged}*{!Form_Discount_Percent_Return_Problem}/100),0)+{!ORC_Shipping_Fee}</expression>
        <scale>2</scale>
    </formulas>
    <formulas>
        <name>Amount_Refund_Shipping_Fee</name>
        <dataType>Currency</dataType>
        <expression>{!ORC_Amount_Charged}*{!Form_Discount_Percent_Shipping_Fee}/100</expression>
        <scale>2</scale>
    </formulas>
    <formulas>
        <name>Amount_Refund_Wrong_Item</name>
        <dataType>Currency</dataType>
        <expression>if({!Form_Discount_Percent_Wrong_Item}&gt;0,{!ORC_Amount_Charged}*{!Form_Discount_Percent_Wrong_Item}/100,0)+ {!ORC_Shipping_Fee}</expression>
        <scale>2</scale>
    </formulas>
    <formulas>
        <name>CC_Amount_Confirmation</name>
        <dataType>String</dataType>
        <expression>If ( {!ORC_CC_Amount_Refund}&gt;0, {!Text_CC_Amount_Confirmation},&quot;&quot;)</expression>
    </formulas>
    <formulas>
        <name>GC_Amount_Confirmation</name>
        <dataType>String</dataType>
        <expression>if({!ORC_New_GC_Amount}&gt;0,{!Text_GC_Amount_Confirmation},&quot;&quot;)</expression>
    </formulas>
    <formulas>
        <name>Gift_Card_Validation</name>
        <dataType>Boolean</dataType>
        <expression>OR(
(OR ({!Form_Gift_Card_Amount_Defective_Item} = &quot;50&quot;,
{!Form_Gift_Card_Amount_Return_Problem} = &quot;50&quot;,
{!Form_Gift_Card_Amount_Shipping_Fee}= &quot;50&quot;,
{!Form_Gift_Card_Amount_Wrong_Item}= &quot;50&quot;) &amp;&amp; {!ORC_Amount_Charged}&lt;300),

(OR ({!Form_Gift_Card_Amount_Defective_Item} = &quot;75&quot;,
{!Form_Gift_Card_Amount_Return_Problem} = &quot;75&quot;,
{!Form_Gift_Card_Amount_Shipping_Fee}= &quot;75&quot;,
{!Form_Gift_Card_Amount_Wrong_Item}= &quot;75&quot;) &amp;&amp; {!ORC_Amount_Charged}&lt;500),

(OR ({!Form_Gift_Card_Amount_Defective_Item} = &quot;100&quot;,
{!Form_Gift_Card_Amount_Return_Problem} = &quot;100&quot;,
{!Form_Gift_Card_Amount_Shipping_Fee}= &quot;100&quot;,
{!Form_Gift_Card_Amount_Wrong_Item}= &quot;100&quot;) &amp;&amp; {!ORC_Amount_Charged}&lt;1000))</expression>
    </formulas>
    <formulas>
        <name>More_30_Days</name>
        <dataType>String</dataType>
        <expression>if(today()-{!ORC_Date_OF_Sale}&lt;30,&quot;&quot;,&quot;ORDER PLACED MORE THAN 30 DAYS AGO&quot;)</expression>
    </formulas>
    <formulas>
        <name>New_Total_Price_Missing_Item</name>
        <dataType>Currency</dataType>
        <expression>{!ORC_Amount_Charged}-({!ORC_Amount_Charged_Item}*{!Quantity_Not_Received_Missing_Item})</expression>
        <scale>2</scale>
    </formulas>
    <formulas>
        <name>New_Total_Price_Price_Adjustment</name>
        <dataType>Currency</dataType>
        <expression> if({!Form_Manual_Adjustment_Price_Adjustment}&gt;1,({!Form_Manual_Adjustment_Price_Adjustment}* {!ORC_Quantity}),0)</expression>
        <scale>2</scale>
    </formulas>
    <formulas>
        <name>New_Total_Price_Price_Match</name>
        <dataType>Currency</dataType>
        <expression>{!Price_Override_Price_Match}*{!ORC_Quantity}</expression>
        <scale>2</scale>
    </formulas>
    <formulas>
        <name>New_Total_Price_Price_Override</name>
        <dataType>Currency</dataType>
        <expression>{!Price_Override_Price_Override}* {!ORC_Quantity}</expression>
        <scale>2</scale>
    </formulas>
    <formulas>
        <name>Percent_Validation</name>
        <dataType>Boolean</dataType>
        <expression>OR ({!Form_Discount_Percent_Shipping_Fee} &gt; 20,
{!Form_Discount_Percent_Defective_Item} &gt; 20, 
{!Form_Discount_Percent_Wrong_Item}&gt;20,
{!Form_Discount_Percent_Return_Problem}&gt; 20)</expression>
    </formulas>
    <formulas>
        <name>Price_Validation</name>
        <dataType>Boolean</dataType>
        <expression>OR ({!Price_Override_Price_Match} &gt; {!ORC_Amount_Charged},
{!Gift_Card_Balance_Gift_Card_Original} &gt; {!ORC_Amount_Charged},
{!Price_Override_Price_Override} &gt; {!ORC_Amount_Charged},
If({!Form_Manual_Adjustment_Promo}&gt;0,{!Form_Manual_Adjustment_Promo},0)+if({!Form_Discount_Percent_Promo}&gt;0,({!ORC_Amount_Charged}*{!Form_Discount_Percent_Promo}/100),0)&gt; {!ORC_Amount_Charged},
If({!Form_Manual_Adjustment_Price_Adjustment}&gt;0,{!Form_Manual_Adjustment_Price_Adjustment},0)+if({!Form_Discount_Percent_Price_Adjustment}&gt;0,({!ORC_Amount_Charged}*{!Form_Discount_Percent_Price_Adjustment}/100),0)+if({!Form_Credit_Shipping_Fee_Price_Adjustment}=TRUE,{!ORC_Shipping_Fee},0)&gt; {!ORC_Amount_Charged},
If({!Form_Manual_Adjustment_Defective_Item}&gt;0,{!Form_Manual_Adjustment_Defective_Item},0)+if({!Form_Discount_Percent_Defective_Item}&gt;0,({!ORC_Amount_Charged}*{!Form_Discount_Percent_Defective_Item}/100),0)+if({!Form_Gift_Card_Amount_Defective_Item}&lt;&gt;&quot;None&quot;,value({!Form_Gift_Card_Amount_Defective_Item}),0)&gt; {!ORC_Amount_Charged})</expression>
    </formulas>
    <formulas>
        <name>Shipping_Fee_Confirmation</name>
        <dataType>String</dataType>
        <expression>if({!ORC_Credit_Shipping_Fee}=TRUE,{!Text_Shipping_Fee_Confirmation},&quot;&quot;)</expression>
    </formulas>
    <formulas>
        <name>Test_Validation</name>
        <dataType>Number</dataType>
        <expression>(If({!Form_Manual_Adjustment_Defective_Item}&gt;0,{!Form_Manual_Adjustment_Defective_Item},0)+if({!Form_Discount_Percent_Defective_Item}&gt;0,({!ORC_Amount_Charged}*{!Form_Discount_Percent_Defective_Item}/100),0))+ if({!Form_Gift_Card_Amount_Defective_Item}&lt;&gt;&quot;None&quot;,value({!Form_Gift_Card_Amount_Defective_Item}),0)</expression>
        <scale>2</scale>
    </formulas>
